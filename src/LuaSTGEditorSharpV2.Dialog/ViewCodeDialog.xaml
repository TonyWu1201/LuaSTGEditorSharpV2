<Window x:Class="LuaSTGEditorSharpV2.Dialog.ViewCodeDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LuaSTGEditorSharpV2.Dialog"
        xmlns:localvm="clr-namespace:LuaSTGEditorSharpV2.Dialog.ViewModel"
        xmlns:ae="http://icsharpcode.net/sharpdevelop/avalonedit"
        xmlns:lex="http://wpflocalizeextension.codeplex.com"
        mc:Ignorable="d"
        Title="{lex:Loc LuaSTGEditorSharpV2.Dialog:Localized:viewCodeDialog_title}" Height="450" Width="800">
    <Grid>
        <ae:TextEditor x:Name="textEditor" FontFamily="Courier New" FontSize="14" ShowLineNumbers="True"/>
    </Grid>
</Window>
